{
  "manifest_version": 3,
  "name": "EquityCheck",
  "version": "1.0",
  "description": "Check if a company is supportive of the LGBTQ+ community",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+G",
        "mac": "Command+G"
      }
    }
  }
}
